{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "ext-soap": "*",
        "alexpechkarev/google-maps": "^10.0",
        "apichef/laravel-obfuscate": "^3.0.0",
        "calebporzio/sushi": "^2.4",
        "crazybooot/base64-validation": "^1.0",
        "cviebrock/eloquent-sluggable": "^9.0",
        "google/apiclient": "2.15.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^3.0",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "3.0",
        "livewire/livewire": "^2.11",
        "opcodesio/log-viewer": "^3.0",
        "power-components/livewire-powergrid": "^3.7",
        "pusher/pusher-php-server": "^7.2",
        "realrashid/sweet-alert": "^6.0",
        "spatie/laravel-permission": "^5.9",
        "verot/class.upload.php": "^2.1",
        "yajra/laravel-datatables-oracle": "^10.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "files": [
            "app/Helper/App.php",
            "app/Helper/StorageHelpers.php",
            "app/Helper/MobitelSmsHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Haruncpi\\LaravelUserActivity\\": "packages/laravel-user-activity/src",
            "ApiChef\\PayHere\\": "packages/laravel-pay-here-master/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
